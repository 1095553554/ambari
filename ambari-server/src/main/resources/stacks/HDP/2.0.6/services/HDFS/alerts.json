{
  "service": [],
  "NAMENODE": [
    {
      "name": "namenode_process",
      "label": "NameNode Process",
      "interval": 1,
      "scope": "any",
      "source": {
        "type": "PORT",
        "uri": "{{hdfs-site/dfs.namenode.http-address}}",
        "default_port": 50070,
        "reporting": {
          "ok": {
            "text": "TCP OK - {0:.4f} response on port {1}"
          },
          "critical": {
            "text": "Connection failed: {0} on host {1}:{2}"
          }
        }        
      }
    }
  ],
  "SECONDARY_NAMENODE": [
